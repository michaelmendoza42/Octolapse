<script type="text/html" id="octolapse-cura-template">
    <div>
        <div>
            <h4>Slicer - Cura</h4>
            <p>
                Match all of settings below with the the settings from Cura exactly.
            </p>
        </div>
        <div>
            <div>
                <h4>
                    Quality
                </h4>
            </div>
            <div class="control-group">
                <label class="control-label" for="octolapse_cura_layer_height">Layer Height</label>
                <div class="controls">
                    <div class="input-append form-inline">
                        <input id="octolapse_cura_layer_height" name="octolapse_cura_layer_height"
                               class="input-small text-right ignore_hidden_errors"
                               data-bind="value: slicers.cura.layer_height"
                               type="number" step="0.0001" min="0.0001" max="100.0" />
                        <span class="add-on">mm</span>
                    </div>
                    <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.layer_height.md" data-help-title="Layer Height" />
                    <div class="error_label_container text-error" data-error-for="octolapse_cura_layer_height"></div>
                </div>
            </div>
            <div>
                <h4>
                    Special Modes
                </h4>
            </div>
            <div class="control-group">
                <label class="control-label" for="octolapse_cura_smooth_spiralized_contours">Spiralize Outer Contour</label>
                <div class="controls">
                    <label class="checkbox">
                        <input id="octolapse_cura_smooth_spiralized_contours" name="octolapse_cura_smooth_spiralized_contours"
                               data-bind="checked: slicers.cura.smooth_spiralized_contours"
                               title="Spiralize Outer Contour"
                               type="checkbox" />Enabled
                        <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.smooth_spiralized_contours.md" data-help-title="Spiralize Outer Contour" />
                    </label>
                    <div class="error_label_container text-error" data-error-for="octolapse_cura_smooth_spiralized_contours"></div>
                </div>
            </div>
            <div class="control-group">
                <label class="control-label" for="octolapse_cura_magic_mesh_surface_mode">Surface Mode</label>
                <div class="controls">
                    <select id="octolapse_cura_magic_mesh_surface_mode" namd="octolapse_cura_magic_mesh_surface_mode"
                            data-bind="options: Octolapse.Printers.profileOptions.cura_surface_mode_options,
                                       optionsText: 'name',
                                       optionsValue: 'value',
                                       value: slicers.cura.magic_mesh_surface_mode"></select>
                    <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.cura_surface_mode.md" data-help-title="Surface Mode" />
                    <div class="error_label_container text-error" data-error-for="octolapse_cura_magic_mesh_surface_mode"></div>
                </div>
            </div>
            <div>
                <h4>
                    Extruder Settings
                </h4>
            </div>
            <div data-bind="foreach: slicers.cura.extruders" class="row-fluid octolapse-table-list-row">
                <div class="span6">
                    <fieldset class="octolapse">
                        <legend data-bind="text: ('Extruder ' + ($index()+1).toString())"></legend>
                        <div>
                            <div>
                                <h5>Material</h5>
                            </div>
                            <div class="control-group">
                                <label class="control-label" data-bind="attr: {'for': ('octolapse_cura_retraction_enable_'+$index().toString())}">Retraction Enabled*</label>
                                <div class="controls">
                                    <label class="checkbox">
                                        <input data-bind="checked: $data.retraction_enable,
                                                          attr: {name: ('octolapse_cura_retraction_enable_'+$index().toString()),
                                                                 id: ('octolapse_cura_retraction_enable_'+$index().toString())}"
                                               title="Enable retraction"
                                               type="checkbox" />Enabled
                                        <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.retraction_enable.md" data-help-title="Retraction Enabled" />
                                    </label>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" data-bind="attr: {'for': ('octolapse_cura_retraction_amount_'+$index().toString())}">Retraction Distance*</label>
                                <div class="controls">
                                    <div class="input-append form-inline">
                                        <input class="input-small text-right ignore_hidden_errors"
                                               data-bind="value: $data.retraction_amount,
                                                          attr: {name: ('octolapse_cura_retraction_amount_'+$index().toString()),
                                                                 id: ('octolapse_cura_retraction_amount_'+$index().toString())}"
                                               type="number" step="0.0001" min="0" max="100.0" required />
                                        <span class="add-on">mm</span>
                                    </div>
                                    <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.retraction_amount.md" data-help-title="Retraction Distance" />
                                    <div class="error_label_container text-error"
                                         data-bind="attr: {'data-error-for': ('octolapse_cura_retraction_amount_'+$index().toString())}">
                                    </div>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" data-bind="attr: {'for': ('octolapse_cura_retraction_retract_speed_'+$index().toString())}">Retraction Retract Speed*</label>
                                <div class="controls">
                                    <div class="input-append form-inline">
                                        <input class="input-small text-right ignore_hidden_errors"
                                               data-bind="value: $data.retraction_retract_speed,
                                                          attr: {name: ('octolapse_cura_retraction_retract_speed_'+$index().toString()),
                                                                 id: ('octolapse_cura_retraction_retract_speed_'+$index().toString())}"
                                               type="number" step="0.0001" min="0" max="9999999" required />
                                        <span class="add-on">mm</span>
                                        <span class="add-on">sec</span>
                                    </div>
                                    <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.retraction_retract_speed.md" data-help-title="Retraction Retract Speed" />
                                    <div class="error_label_container text-error"
                                         data-bind="attr: {'data-error-for': ('octolapse_cura_retraction_retract_speed_'+$index().toString())}">
                                    </div>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" data-bind="attr: {'for': ('octolapse_cura_retraction_prime_speed_'+$index().toString())}">Retraction Prime Speed*</label>
                                <div class="controls">
                                    <div class="input-append form-inline">
                                        <input class="input-small text-right ignore_hidden_errors"
                                               data-bind="value: $data.retraction_prime_speed,
                                                          attr: {name: ('octolapse_cura_retraction_prime_speed_'+$index().toString()),
                                                                 id: ('octolapse_cura_retraction_prime_speed_'+$index().toString())}"
                                               type="number" step="0.0001" min="0" max="9999999" required />
                                        <span class="add-on">mm</span>
                                        <span class="add-on">sec</span>
                                    </div>
                                    <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.retraction_prime_speed.md" data-help-title="Retraction Prime Speed" />
                                    <div class="error_label_container text-error"
                                         data-bind="attr: {'data-error-for': ('octolapse_cura_retraction_prime_speed_'+$index().toString())}">
                                    </div>
                                </div>
                            </div>
                            <div>
                                <h5>Speed</h5>
                            </div>
                            <div class="control-group">
                                <label class="control-label" data-bind="attr: {'for': ('octolapse_cura_speed_travel_'+$index().toString())}">Travel Speed*</label>
                                <div class="controls">
                                    <div class="input-append form-inline">
                                        <input class="input-small text-right ignore_hidden_errors"
                                               data-bind="value: $data.speed_travel,
                                                          attr: {name: ('octolapse_cura_speed_travel_'+$index().toString()),
                                                                 id: ('octolapse_cura_speed_travel_'+$index().toString())}"
                                               type="number" step="0.0001" min="0" max="9999999" required />
                                        <span class="add-on">mm</span>
                                        <span class="add-on">sec</span>
                                    </div>
                                    <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.speed_travel.md" data-help-title="Travel Speed" />
                                    <div class="error_label_container text-error"
                                         data-bind="attr: {'data-error-for': ('octolapse_cura_speed_travel_'+$index().toString())}">
                                    </div>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" data-bind="attr: {'for': ('octolapse_cura_max_feedrate_z_override_'+$index().toString())}">Maximum Z Speed</label>
                                <div class="controls">
                                    <div class="input-append form-inline">
                                        <input class="input-small text-right ignore_hidden_errors"
                                               data-bind="value: $data.max_feedrate_z_override,
                                                          attr: {name: ('octolapse_cura_max_feedrate_z_override_'+$index().toString()),
                                                                 id: ('octolapse_cura_max_feedrate_z_override_'+$index().toString())}"
                                               type="number" step="0.0001" min="0" max="9999999" />
                                        <span class="add-on">mm</span>
                                        <span class="add-on">sec</span>
                                    </div>
                                    <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.max_feedrate_z_override.md" data-help-title="Maximum Z Speed" />
                                    <div class="error_label_container text-error"
                                         data-bind="attr: {'data-error-for': ('octolapse_cura_max_feedrate_z_override_'+$index().toString())}">
                                    </div>
                                </div>
                            </div>
                            <div>
                                <h5>Travel</h5>
                            </div>
                            <div class="control-group">
                                <label class="control-label" data-bind="attr: {'for': ('octolapse_cura_retraction_hop_enabled_'+$index().toString())}">Z Hop When Retracted*</label>
                                <div class="controls">
                                    <label class="checkbox">
                                        <input data-bind="checked: $data.retraction_hop_enabled,
                                                          attr: {name: ('octolapse_cura_retraction_hop_enabled_'+$index().toString()),
                                                                 id: ('octolapse_cura_retraction_hop_enabled_'+$index().toString())}"
                                               title="Enable retraction"
                                               type="checkbox" />Enabled
                                        <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.retraction_hop_enabled.md" data-help-title="Z Hop When Retracted" />
                                    </label>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label" data-bind="attr: {'for': ('octolapse_cura_retraction_hop_'+$index().toString())}">Z Hop Height*</label>
                                <div class="controls">
                                    <div class="input-append form-inline">
                                        <input class="input-small text-right ignore_hidden_errors"
                                               data-bind="value: $data.retraction_hop,
                                                          attr: {name: ('octolapse_cura_retraction_hop_'+$index().toString()),
                                                                 id: ('octolapse_cura_retraction_hop_'+$index().toString())}"
                                               type="number" step="0.0001" min=".00" max="100.0" required />
                                        <span class="add-on">mm</span>
                                    </div>
                                    <a class="octolapse_help" data-help-url="profiles.printer.slicers.cura.retraction_hop.md" data-help-title="Z Hop Height" />
                                    <div class="error_label_container text-error"
                                         data-bind="attr: {'data-error-for': ('octolapse_cura_retraction_hop_'+$index().toString())}">
                                    </div>
                                </div>
                            </div>
                            <div class="control-group">
                                <div class="controls">
                                    <span><i>* Indicates required fields</i></span>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </div>
</script>
